<Application x:Class="CrazyFord.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/CrazyFord.CommonControls;component/Themes/Brushes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/CrazyFord.CommonControls;component/Themes/Button.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/CrazyFord.CommonControls;component/Themes/Label.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/CrazyFord.CommonControls;component/Themes/Window.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
